				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li MayRequire="ceteam.combatextended">Hands</li>
				<li MayRequire="ceteam.combatextended">Feet</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<comps>
			<li MayRequire="kentington.saveourship2">
				<compClass>SaveOurShip2.CompEVA</compClass>
			</li>
		</comps>
		<modExtensions>
			<li Class="VEF.Apparels.ApparelExtension">
				<isUnifiedApparel>true</isUnifiedApparel>
				<hideHead>false</hideHead>
			</li>
			<li Class="CombatExtended.PartialArmorExt" MayRequire="ceteam.combatextended">
				<stats>
					<li>
						<ArmorRating_Sharp>0.8</ArmorRating_Sharp>
						<parts>
							<li>Hand</li>
						</parts>
					</li>
					<li>
						<ArmorRating_Blunt>0.8</ArmorRating_Blunt>
						<parts>
							<li>Hand</li>
						</parts>
					</li>
				</stats>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="GW_Rulers_ArmorBase">
		<defName>GW_Rulers_EmperorArmorW</defName>
		<label>Emperor armor (W)</label>
		<description>BETA BETA BETA BETA</description>
		<graphicData>
			<texPath>Emperor/GW_HRM_Emperor_ArmorW_south</texPath>
		</graphicData>
		<recipeMaker Inherit="false" IsNull="True"/>
			<!-- <researchPrerequisite>GW_Rulers_OneTrueArmor</researchPrerequisite> -->
		<!-- </recipeMaker> -->
		<equippedStatOffsets>
			<InjuryHealingFactor>2</InjuryHealingFactor>
			<PsychicSensitivity>2</PsychicSensitivity>
			<PsychicSensitivityOffset MayRequire="ludeon.rimworld.royalty">0.20</PsychicSensitivityOffset>
      		<PsychicEntropyRecoveryRate MayRequire="ludeon.rimworld.royalty">0.090</PsychicEntropyRecoveryRate>
		</equippedStatOffsets>
		<statBases>
			<WorkToMake>240000</WorkToMake>
			<MaxHitPoints>1200</MaxHitPoints>
			<MarketValue>7800</MarketValue>
			<Mass>11</Mass>
			<Flammability>0.0</Flammability>
			<ArmorRating_Sharp>1.65</ArmorRating_Sharp>
			<ArmorRating_Blunt>1.3</ArmorRating_Blunt>
			<ArmorRating_Heat>1.5</ArmorRating_Heat>
			<Insulation_Cold>40</Insulation_Cold>
			<Insulation_Heat>14</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<tickerType>Normal</tickerType> 
		<costList>
			<Plasteel>150</Plasteel>
			<Gold>12</Gold>
			<HP_Ceramite>200</HP_Ceramite>
			<HP_Adamantium>50</HP_Adamantium>
			<HP_Auramite>60</HP_Auramite>
			<GW_ComponentRelic>7</GW_ComponentRelic>
			<ComponentSpacer>14</ComponentSpacer>
			<ComponentIndustrial>20</ComponentIndustrial>
		</costList>
		<apparel>
			<wornGraphicPath>Emperor/GW_HRM_Emperor_ArmorW</wornGraphicPath>
			<tags>
				<li>GW_ChapterMaster</li>
			</tags>
			<drawData>
				<scale>1.2</scale>
			</drawData>
		</apparel>
		<modExtensions>
			<li Class="GW_ApparelRestriction.DefModExtension_ApparelRestriction">
				<isRestrictStat>true</isRestrictStat>
				<statDef>PsychicSensitivity</statDef>
				<reqMode>GreaterEqual</reqMode>
				<value>1.4</value>
			</li>
		</modExtensions>
		<comps>
			<li Class="GW4KArmor.CompProperties_TriColorMask">
				<maskCount>1</maskCount>
				<palettePresets>GW_CS_BodyChapterPalettesCustodes</palettePresets>
				<defaultPalette>
					<colorA>(250, 176, 67)</colorA>
					<colorB>(250, 176, 67)</colorB>
					<colorC>(180, 65, 74)</colorC>
				</defaultPalette>
			</li>
			<li Class="BM_ApparelSet.CompProperties_ApparelSet">
				<apparelSetEffect>
					<allApparels>
						<li>GW_Rulers_EmperorArmorW</li>
						<li>GW_Rulers_EmperorShoulderW</li>
						<li>GW_Rulers_EmperorHelmetW</li>
					</allApparels>
					<hediff>GW_Ruler_EmperorAura</hediff>
				</apparelSetEffect>
			</li>
		</comps>
	</ThingDef>
	
