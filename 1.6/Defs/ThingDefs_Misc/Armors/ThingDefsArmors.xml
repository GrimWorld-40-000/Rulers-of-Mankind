	<ThingDef Abstract="true" Name="GW_Rulers_ArmorBase" ParentName="ArmorMachineableBase">
		<techLevel>Spacer</techLevel>
                                                        <!-- These recipes should use autoforging and cant use the usual recipe maker -->
		<recipeMaker Inherit="false" IsNull="True"/>
			<!-- <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef> -->
			<!-- <skillRequirements> -->
				<!-- <Crafting>20</Crafting> -->
				<!-- <Intellectual>15</Intellectual> -->
			<!-- </skillRequirements> -->
			<!-- <recipeUsers Inherit="false"> -->
				<!-- <li>GW_Autoforge</li> -->
			<!-- </recipeUsers> -->
		<!-- </recipeMaker> -->
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1</drawSize>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<uiIconScale>1</uiIconScale>
		<equippedStatOffsets>
			<Beauty>50</Beauty>			
			<DecompressionResistance MayRequire="kentington.saveourship2">0.75</DecompressionResistance>
			<VacuumSpeedMultiplier MayRequire="kentington.saveourship2">4</VacuumSpeedMultiplier>
			<VacuumResistance MayRequire="ludeon.rimworld.odyssey">0.6</VacuumResistance>
		</equippedStatOffsets>
		<apparel>
			<drawData>
				<dataNorth>
					<offset>(0.0, 0.0, 0.0)</offset>
				</dataNorth>
				<dataWest>
					<offset>(0.0, 0.0, -0.1)</offset>
				</dataWest>
				<dataEast>
					<offset>(0.0, 0.0, -0.1)</offset>
				</dataEast>
