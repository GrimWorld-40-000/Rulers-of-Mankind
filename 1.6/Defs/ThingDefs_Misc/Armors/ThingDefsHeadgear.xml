<Defs>

	<ThingDef Abstract="true" Name="GW_Rulers_HelmetBase" ParentName="ArmorHelmetMakeableBase">
		<techLevel>Spacer</techLevel>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1</drawSize>
		</graphicData>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>20</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>GW_Autoforge</li>
			</recipeUsers>
		</recipeMaker>
		<uiIconScale>1.6</uiIconScale>
		<equippedStatOffsets>
			<ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
			<PawnBeauty>50</PawnBeauty>
			<VacuumResistance MayRequire="ludeon.rimworld.odyssey">0.6</VacuumResistance>
		</equippedStatOffsets>
		<statBases>
			<Flammability>0.0</Flammability>
		</statBases>
		<apparel>
			
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
				<li>PrestigeCombatGear</li>
			</tags>
			<drawData>
				<scale>1.1</scale>
				<childScale>1.1</childScale>
			</drawData>
			<useWornGraphicMask>true</useWornGraphicMask>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<layers>
				<li>EyeCover</li>
				<li MayRequire="ceteam.combatextended">StrappedHead</li>
				<li MayRequire="ceteam.combatextended">OnHead</li>
				<li>Overhead</li> <!--Needs to be last for the tool to properly label this a helmet-->
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<modExtensions>
			<li Class="VEF.Apparels.ApparelExtension">
				<isUnifiedApparel>true</isUnifiedApparel>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="GW_Rulers_HelmetBase">
		<defName>GW_Rulers_EmperorHelmetW</defName>
		<label>Beta Emperor helmet(W)</label>
		<description>WIP.</description>
		<graphicData>
			<texPath>Emperor/Headgear/GW_HRM_Emperor_LaurelsW</texPath>
		</graphicData>
		<apparel>
			<renderSkipFlags>
				<li>None</li>
			</renderSkipFlags>
			<tags>
			</tags>
			<wornGraphicPath>Emperor/Headgear/GW_HRM_Emperor_LaurelsW</wornGraphicPath>
			<drawData>
				<scale>1.1</scale>
				<dataSouth>
					<offset>(0, 0, 0.05)</offset>
				</dataSouth>
				<dataNorth>
					<offset>(0, 0, 0.05)</offset>
				</dataNorth>
				<dataEast>
					<offset>(0, 0, 0.05)</offset>
				</dataEast>
				<dataWest>
					<offset>(0, 0, 0.05)</offset>
				</dataWest>
			</drawData>
		</apparel>
		<equippedStatOffsets>
			<PainShockThreshold>0.05</PainShockThreshold>
			<ToxicEnvironmentResistance>0.75</ToxicEnvironmentResistance>
			<DecompressionResistance MayRequire="kentington.saveourship2">0.25</DecompressionResistance>
			<HypoxiaResistance MayRequire="kentington.saveourship2">1</HypoxiaResistance>
		</equippedStatOffsets>
		<costList>
			<GW_Ruler_Template>1</GW_Ruler_Template>
			<ComponentSpacer>5</ComponentSpacer>
			<ComponentIndustrial>12</ComponentIndustrial>
			<Plasteel>110</Plasteel>
			<HP_Ceramite>80</HP_Ceramite>			
			<HP_Auramite>10</HP_Auramite>
		</costList>
		<statBases>
			<WorkToMake>40000</WorkToMake>
			<MaxHitPoints>650</MaxHitPoints>
			<MarketValue>1000</MarketValue>
			<Mass>2</Mass>
			<ArmorRating_Sharp>1.30</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
			<ArmorRating_Heat>0.70</ArmorRating_Heat>
			<Insulation_Cold>8</Insulation_Cold>
			<Insulation_Heat>4</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<recipeMaker>
			<researchPrerequisite>Fabrication</researchPrerequisite>
		</recipeMaker>
		<comps>
			<li Class="GW4KArmor.CompProperties_TriColorMask">
				<maskCount>1</maskCount>
				<palettePresets>GW_CS_HelmetChapterPalettesCustodes</palettePresets>
				<defaultPalette>
					<colorA>(250, 176, 67)</colorA>
					<colorB>(250, 176, 67)</colorB>
					<colorC>(180, 65, 74)</colorC>
				</defaultPalette>
			</li>
			<li MayRequire="kentington.saveourship2">
				<compClass>SaveOurShip2.CompEVA</compClass>
			</li>
		</comps>
	</ThingDef>
	
